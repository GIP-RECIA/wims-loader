        <!-- Style pour impression -->
        <link rel="stylesheet" media="print" href="html/themes/_css/print.css">

        {% if entNetocentre %}
        {# d√©but ajout bandeau et footer ENT #}
        <style>
            extended-uportal-footer {
                display: none;
            }
            @media only screen and (hover: hover) and (pointer: fine) {
                body {
                    height: calc(calc(100% - var(--recia-header-height, 68px)) - 10px);
                }

                extended-uportal-footer {
                    display: unset;
                    position: fixed;
                    bottom: 0;
                    right: 0;
                    left: 0;
                    transition: transform .2s;
                }

                extended-uportal-header:hover ~ extended-uportal-footer,
                extended-uportal-footer:hover {
                    transform: translateY(0) !important;
                }

                body:hover extended-uportal-footer {
                    transform: translateY(calc(100% - 10px));
                }
            }
        </style>
        {# fin ajout bandeau et footer ENT #}
        {% endif %}

        {% block style %}
        {% endblock %}

        {% block javascripts %}
            {% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}